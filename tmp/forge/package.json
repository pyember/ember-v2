{
  "name": "forge",
  "version": "0.1.0",
  "description": "A powerful coding assistant that orchestrates multiple AI models for optimal results",
  "bin": {
    "forge": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "forge": "tsx src/cli/index.ts",
    "install-global": "npm run build && npm link",
    "demo": "tsx demo/real-integration.ts",
    "benchmark": "tsx benchmarks/routing-performance.ts"
  },
  "keywords": [
    "ai",
    "coding-assistant", 
    "llm",
    "ember",
    "codex"
  ],
  "author": "Ember Team",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "eventsource-parser": "^1.1.2",
    "ink": "^4.4.1",
    "js-yaml": "^4.1.0",
    "openai": "^4.24.0",
    "ora": "^7.0.1",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@ember-ai/ember": ">=1.0.0"
  },
  "peerDependenciesMeta": {
    "@ember-ai/ember": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}