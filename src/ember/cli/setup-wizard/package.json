{
  "name": "@ember-ai/setup",
  "version": "1.0.0",
  "description": "Delightful setup wizard for Ember AI",
  "type": "module",
  "bin": {
    "ember-setup": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
